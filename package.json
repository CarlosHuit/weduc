{
  "name": "weduc",
  "version": "0.0.0",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "start-dev": "concurrently -r \"npm run start:server-dev\"  \"npm run start:client-dev\"",
    "start:mobile": "concurrently -r \"npm run start:server-dev\"  \"npm run start:client-mobile\"",
    "start:production": "concurrently -r \"npm run start:server-prod\" \"npm run start:client-prod\" ",
    "start:client-dev": "ng serve --port 4540",
    "start:server-dev": "NODE_ENV=development DEBUG=WEDUC:* nodemon server/index.js --exec babel-node --presets es2015,stage-0 --plugins transform-async-to-generator",
    "start:server-prod": "NODE_ENV=production node server.js",
    "start:client-prod": "ng serve --prod",
    "start:client-mobile": "ng serve --host 192.168.43.77",
    "build": "concurrently -r \"npm run build:client \" \"npm run build:server\"",
    "build:client": "ng build --prod --base-href 'https://carloshuit.github.io/weduc/' ",
    "build:server": "babel server --out-dir dist/server",
    "postinstall": "concurrently -r \"ng build --prod\" \"npm run build:server\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/cdk": "^7.0.1",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "^7.0.1",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/pwa": "^0.10.2",
    "@angular/router": "~7.0.0",
    "@angular/service-worker": "~7.0.0",
    "@auth0/angular-jwt": "^2.0.0",
    "async-to-generator": "^1.1.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "concurrently": "^4.0.1",
    "core-js": "^2.5.7",
    "debug": "^4.1.0",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.5",
    "mongoose-unique-validator": "^2.0.2",
    "nodemon": "^1.18.7",
    "path": "^0.12.7",
    "rxjs": "^6.3.3",
    "sweet-scroll": "^3.0.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.1",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "enhanced-resolve": "^3.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  }
}
